inputs: tests-examples/cp19/tables/21-1.txt, tests-examples/cp19/tables/21-2.txt, tests-examples/cp19/tables/21-3.txt
output: tests-examples/cp19/tables/21.out
const: "red", "green"
aggrs:
attrs: "color"
bools: "=="
loc: 1

