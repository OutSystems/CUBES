Joining, by = "age"
SELECT `id`,
       `age`,
       `n`
FROM
  (SELECT `id`,
          `age`,
          `n`
   FROM `input0` AS `LHS`
   LEFT JOIN
     (SELECT `age`,
             COUNT() AS `n`
      FROM `input0`
      GROUP BY `age`) AS `RHS` ON (`LHS`.`age` = `RHS`.`age`))

