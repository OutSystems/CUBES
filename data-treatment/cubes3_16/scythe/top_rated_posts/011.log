SELECT `VehicleID`,
       `Name`,
       `concatCity`
FROM
  (SELECT `VehicleID`,
          `Name`,
          `concatCity`
   FROM `input1` AS `LHS`
   LEFT JOIN
     (SELECT `VehicleID`,
             string_agg(`City`, ', ') AS `concatCity`
      FROM `input2`
      GROUP BY `VehicleID`) AS `RHS` ON (`LHS`.`VehicleID` = `RHS`.`VehicleID`))
