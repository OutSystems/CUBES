con <- DBI::dbConnect(RSQLite::SQLite(), ":memory:")
input1 <- read_csv("tests-examples/scythe/top_rated_posts/tables/i037.csv", col_types = cols(c1 = col_integer(),c2 = col_character(),c3 = col_character()))
input1 <- copy_to(con, input1)
