+++++++++++++++++++++++++++++++++++++ SQL Solution +++++++++++++++++++++++++++++++++++++

SELECT `c1`,
       `c2`,
       `c3`
FROM
  (SELECT `c1`,
          `c2`,
          `c3`
   FROM `input1` AS `LHS`
   INNER JOIN
     (SELECT `c2`,
             MIN(`c1`) AS `c1`
      FROM `input1`
      GROUP BY `c2`) AS `RHS` ON (`LHS`.`c1` = `RHS`.`c1`
                                  AND `LHS`.`c2` = `RHS`.`c2`))
