+++++++++++++++++++++++++++++++++++++ SQL Solution +++++++++++++++++++++++++++++++++++++

SELECT `CName`,
       `AddressID` AS `minAddressID`,
       `AddressLine`
FROM
  (SELECT `CName`,
          `AddressID`,
          `AddressLine`
   FROM
     (SELECT `CName`,
             MIN(`AddressID`) AS `AddressID`
      FROM `input1`
      GROUP BY `CName`) AS `LHS`
   LEFT JOIN `input1` AS `RHS` ON (`LHS`.`CName` = `RHS`.`CName`
                                   AND `LHS`.`AddressID` = `RHS`.`AddressID`))
