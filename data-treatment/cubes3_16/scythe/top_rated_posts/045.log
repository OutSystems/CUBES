con <- DBI::dbConnect(RSQLite::SQLite(), ":memory:")
input1 <- read_csv("tests-examples/scythe/top_rated_posts/tables/i045.csv", col_types = cols(user_id = col_integer(),account_no = col_integer(),zip = col_integer(),date = col_character()))
input1$date <- dmy(input1$date)
input1 <- copy_to(con, input1)
expected_output <- read_csv("tests-examples/scythe/top_rated_posts/tables/o045.csv", col_types = cols(user_id = col_integer(),n = col_integer()))

df2 <- input1 %>% group_by(date, user_id) %>% summarise(n = n()) %>% ungroup()
df3 <- inner_join(df2, input1, by=c('n' = 'user_id'), suffix = c('', '.other'))
df4 <- df3 %>% filter(n > 1)
out <- df4 %>% select(user_id, n) %>% distinct()


+++++++++++++++++++++++++++++++++++++ SQL Solution +++++++++++++++++++++++++++++++++++++

SELECT DISTINCT `user_id`,
                `n`
FROM
  (SELECT `date`,
          `user_id`,
          `n`,
          `account_no`,
          `zip`,
          `date.other`
   FROM
     (SELECT `date`,
             `user_id`,
             COUNT() AS `n`
      FROM `input1`
      GROUP BY `date`,
               `user_id`) AS `LHS`
   INNER JOIN `input1` AS `RHS` ON (`LHS`.`n` = `RHS`.`user_id`))
WHERE (`n` > 1.0)
