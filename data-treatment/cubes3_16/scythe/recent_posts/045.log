+++++++++++++++++++++++++++++++++++++ SQL Solution +++++++++++++++++++++++++++++++++++++

SELECT `Name` AS `MenuName`,
       `sumQuantity` AS `Quantity`
FROM
  (SELECT `Menu_id`,
          `Name`,
          SUM(`Quantity`) AS `sumQuantity`
   FROM
     (SELECT `Menu_id`,
             `Name`,
             `ID`,
             `Quantity`
      FROM `input1` AS `LHS`
      LEFT JOIN `input2` AS `RHS` ON (`LHS`.`Menu_id` = `RHS`.`Menu_id`))
   GROUP BY `Menu_id`,
            `Name`)
