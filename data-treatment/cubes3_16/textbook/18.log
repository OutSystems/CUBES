SELECT `P_id`,
       `S_name`
FROM
  (SELECT `S_key`,
          `P_id`,
          `cost`,
          `S_name`
   FROM
     (SELECT `S_key`,
             `P_id`,
             `cost`,
             `S_name`
      FROM `input1` AS `LHS`
      INNER JOIN `input2` AS `RHS` ON (`LHS`.`S_key` = `RHS`.`S_key`)) AS `LHS`
   INNER JOIN
     (SELECT `P_id`,
             MAX(`cost`) AS `cost`
      FROM `input1`
      GROUP BY `P_id`) AS `RHS` ON (`LHS`.`P_id` = `RHS`.`P_id`
                                    AND `LHS`.`cost` = `RHS`.`cost`))
