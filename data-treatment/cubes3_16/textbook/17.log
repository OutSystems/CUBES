SELECT `P_id`,
       `S_name`
FROM
  (SELECT `P_id`,
          `meancost`,
          `S_key`,
          `cost`,
          `S_name`
   FROM
     (SELECT `P_id`,
             `meancost`,
             `S_key`,
             `cost`
      FROM
        (SELECT `P_id`,
                AVG(`cost`) AS `meancost`
         FROM `input1`
         GROUP BY `P_id`) AS `LHS`
      INNER JOIN `input1` AS `RHS` ON (`LHS`.`P_id` = `RHS`.`P_id`)) AS `LHS`
   INNER JOIN `input2` AS `RHS` ON (`LHS`.`S_key` = `RHS`.`S_key`))
WHERE (`meancost` < `cost`)
