SELECT `Name` AS `Department`,
       `Name.other` AS `Employee`,
       `Salary`
FROM
  (SELECT `Id`,
          `Name`,
          `Id.other`,
          `Name.other`,
          `Salary`
   FROM `input2` AS `LHS`
   INNER JOIN
     (SELECT `Id`,
             `Name`,
             `Salary`,
             `DepartmentId`
      FROM `input1` AS `LHS`
      INNER JOIN
        (SELECT `DepartmentId`,
                MAX(`Salary`) AS `Salary`
         FROM `input1`
         GROUP BY `DepartmentId`) AS `RHS` ON (`LHS`.`Salary` = `RHS`.`Salary`
                                               AND `LHS`.`DepartmentId` = `RHS`.`DepartmentId`)) AS `RHS` ON (`LHS`.`Id` = `RHS`.`DepartmentId`))
