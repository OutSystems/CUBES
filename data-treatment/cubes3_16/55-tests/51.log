SELECT DISTINCT `S_name`,
                `n`
FROM
  (SELECT `S_key`,
          `S_name`,
          `P_id`,
          `color`,
          `n`
   FROM
     (SELECT `S_key`,
             `S_name`,
             `P_id`,
             `color`
      FROM
        (SELECT `S_key`,
                `S_name`,
                `P_id`
         FROM `input3` AS `LHS`
         INNER JOIN `input1` AS `RHS` ON (`LHS`.`S_key` = `RHS`.`S_key`)) AS `LHS`
      INNER JOIN `input2` AS `RHS` ON (`LHS`.`P_id` = `RHS`.`P_id`)) AS `LHS`
   LEFT JOIN
     (SELECT `P_id`,
             `color`,
             COUNT() AS `n`
      FROM
        (SELECT `S_key`,
                `S_name`,
                `P_id`,
                `color`
         FROM
           (SELECT `S_key`,
                   `S_name`,
                   `P_id`
            FROM `input3` AS `LHS`
            INNER JOIN `input1` AS `RHS` ON (`LHS`.`S_key` = `RHS`.`S_key`)) AS `LHS`
         INNER JOIN `input2` AS `RHS` ON (`LHS`.`P_id` = `RHS`.`P_id`))
      GROUP BY `P_id`,
               `color`) AS `RHS` ON (`LHS`.`P_id` = `RHS`.`P_id`
                                     AND `LHS`.`color` = `RHS`.`color`))
