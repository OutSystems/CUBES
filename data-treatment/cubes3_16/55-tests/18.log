SELECT `P_id`,
       `S_name`
FROM
  (SELECT `P_id`,
          `cost`,
          `S_key`,
          `S_name`
   FROM
     (SELECT `P_id`,
             `cost`,
             `S_key`
      FROM
        (SELECT `P_id`,
                MAX(`cost`) AS `cost`
         FROM `input1`
         GROUP BY `P_id`) AS `LHS`
      INNER JOIN `input1` AS `RHS` ON (`LHS`.`P_id` = `RHS`.`P_id`
                                       AND `LHS`.`cost` = `RHS`.`cost`)) AS `LHS`
   INNER JOIN `input2` AS `RHS` ON (`LHS`.`S_key` = `RHS`.`S_key`))
