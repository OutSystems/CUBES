SELECT `P_id`,
       `S_name`
FROM
  (SELECT `P_id`,
          `S_name`,
          `S_key`,
          `cost`,
          `meancost`
   FROM
     (SELECT `P_id`,
             `S_name`,
             `S_key`,
             `cost`
      FROM `input2` AS `LHS`
      INNER JOIN `input1` AS `RHS` ON (`LHS`.`P_id` = `RHS`.`P_id`)) AS `LHS`
   INNER JOIN
     (SELECT `P_id`,
             AVG(`cost`) AS `meancost`
      FROM `input1`
      GROUP BY `P_id`) AS `RHS` ON (`LHS`.`P_id` = `RHS`.`P_id`))
WHERE (`meancost` >= `cost`)
