SELECT `S_name`,
       `n`
FROM
  (SELECT `S_name`,
          COUNT() AS `n`
   FROM
     (SELECT `P_id`,
             `S_name`,
             `meancost`
      FROM `input2` AS `LHS`
      INNER JOIN
        (SELECT `P_id`,
                AVG(`cost`) AS `meancost`
         FROM `input1`
         GROUP BY `P_id`) AS `RHS` ON (`LHS`.`P_id` = `RHS`.`P_id`))
   GROUP BY `S_name`)
