SELECT `P_id`,
       `S_name`,
       `n`
FROM
  (SELECT `S_key`,
          `P_id`,
          `cost`,
          `S_name`,
          `n`
   FROM `input1` AS `LHS`
   INNER JOIN
     (SELECT `S_key`,
             `S_name`,
             COUNT() AS `n`
      FROM `input2`
      GROUP BY `S_key`,
               `S_name`) AS `RHS` ON (`LHS`.`S_key` = `RHS`.`S_key`))
WHERE (`S_name` != 'SN1')
