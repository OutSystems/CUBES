SELECT `P_id`,
       `n`
FROM
  (SELECT `P_id`,
          COUNT() AS `n`
   FROM
     (SELECT *
      FROM
        (SELECT `P_id`,
                `color`,
                `S_key`,
                `S_name`
         FROM
           (SELECT `P_id`,
                   `color`,
                   `S_key`
            FROM `input2` AS `LHS`
            INNER JOIN `input1` AS `RHS` ON (`LHS`.`P_id` = `RHS`.`P_id`)) AS `LHS`
         INNER JOIN `input3` AS `RHS` ON (`LHS`.`S_key` = `RHS`.`S_key`))
      WHERE (`color` = 'red'
             AND `S_key` = 'S5'))
   GROUP BY `P_id`)
