SELECT `eid`,
       `aname`
FROM
  (SELECT `eid`,
          `n`,
          `aid`,
          `aname`,
          `crusingrange`
   FROM
     (SELECT `eid`,
             `n`,
             `aid`
      FROM
        (SELECT `eid`,
                COUNT() AS `n`
         FROM `input2`
         GROUP BY `eid`) AS `LHS`
      INNER JOIN `input2` AS `RHS` ON (`LHS`.`eid` = `RHS`.`eid`)) AS `LHS`
   INNER JOIN `input1` AS `RHS` ON (`LHS`.`aid` = `RHS`.`aid`))
