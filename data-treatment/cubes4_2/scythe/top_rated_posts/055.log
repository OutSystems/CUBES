SELECT `User`,
       `Activity`,
       `concatPageURL`
FROM
  (SELECT `Activity`,
          `User`,
          string_agg(`PageURL`, ' ') AS concatPageURL
   FROM input1
   GROUP BY Activity,
            USER)
ORDER BY USER
